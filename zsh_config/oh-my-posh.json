{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#5fb3ff",
          "template": "{{ .Env.POSH_PATH }} "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ .Env.POSH_GIT_COLOR }}"
          ],
          "template": "{{ if .Env.POSH_GIT_BRANCH }}{{ .Env.POSH_GIT_BRANCH }} {{ end }}"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#c0c0c0",
          "template": "{{ if .Env.POSH_ICON }}{{ .Env.POSH_ICON }} {{ end }}"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
