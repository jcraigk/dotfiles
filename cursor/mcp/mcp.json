{
  "mcpServers": {
    "atlassian": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--env-file",
        "~/dotfiles/cursor/mcp/atlassian.env",
        "ghcr.io/sooperset/mcp-atlassian:latest"
      ]
    },
    "slack": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--env-file",
        "~/dotfiles/cursor/mcp/slack.env",
        "ghcr.io/korotovsky/slack-mcp-server:latest",
        "mcp-server",
        "--transport",
        "stdio"
      ]
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--env-file",
        "~/dotfiles/cursor/mcp/github.env",
        "ghcr.io/github/github-mcp-server:latest"
      ]
    },
    "buildkite-official-local": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--env-file",
        "~/dotfiles/cursor/mcp/buildkite.env",
        "ghcr.io/buildkite/buildkite-mcp-server",
        "stdio"
      ]
    },
    "glean": {
      "type": "http",
      "url": "https://gusto-be.glean.com/mcp/default"
    },
    "redash": {
      "command": "npx",
      "args": ["-y", "@suthio/redash-mcp"],
      "env": {
        "REDASH_API_KEY": "<SECRET>",
        "REDASH_URL": "https://redash.zp-int.com"
      }
    }
  }
}
